{
  "name": "supermolt-backend",
  "version": "0.3.4",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "start": "bunx prisma generate && bunx prisma db push --accept-data-loss && bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "db:generate": "bunx prisma generate",
    "db:push": "bunx prisma db push",
    "db:migrate": "bunx prisma migrate dev",
    "db:studio": "bunx prisma studio",
    "postinstall": "~/.bun/bin/bunx prisma generate || true"
  },
  "dependencies": {
    "@hono/swagger-ui": "^0.5.3",
    "@prisma/client": "^6.0.0",
    "@privy-io/node": "^0.9.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "@types/node": "^25.2.0",
    "bs58": "^6.0.0",
    "bullmq": "^5.10.3",
    "clanker-sdk": "^4.2.10",
    "cron": "^4.4.0",
    "ethers": "^6.16.0",
    "expo-server-sdk": "^6.0.0",
    "gray-matter": "^4.0.3",
    "hono": "^4.4.0",
    "hono-rate-limiter": "^0.5.3",
    "ioredis": "^5.4.0",
    "jose": "^5.6.0",
    "kalshi-typescript": "^3.6.0",
    "pg": "^8.18.0",
    "prom-client": "^15.1.3",
    "siwe": "^3.0.0",
    "socket.io": "^4.8.3",
    "tweetnacl": "^1.0.3",
    "viem": "^2.45.2",
    "ws": "^8.19.0",
    "yaml": "^2.8.2",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "@openzeppelin/contracts-upgradeable": "^5.4.0",
    "@types/bun": "^1.1.0",
    "@types/ws": "^8.18.1",
    "prisma": "^6.0.0",
    "socket.io-client": "^4.8.3",
    "solc": "0.8.24",
    "typescript": "^5.5.0"
  }
}
