{
  "name": "supermolt-backend",
  "version": "0.3.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "start": "bunx prisma generate && bunx prisma db push --accept-data-loss && bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "db:generate": "bunx prisma generate",
    "db:push": "bunx prisma db push",
    "db:migrate": "bunx prisma migrate dev",
    "db:studio": "bunx prisma studio",
    "postinstall": "bunx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "@privy-io/server-auth": "^1.7.0",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "@types/node": "^25.2.0",
    "bs58": "^6.0.0",
    "gray-matter": "^4.0.3",
    "hono": "^4.4.0",
    "hono-rate-limiter": "^0.5.3",
    "ioredis": "^5.4.0",
    "jose": "^5.6.0",
    "pg": "^8.18.0",
    "socket.io": "^4.8.3",
    "tweetnacl": "^1.0.3",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "prisma": "^6.0.0",
    "socket.io-client": "^4.8.3",
    "typescript": "^5.5.0"
  }
}
