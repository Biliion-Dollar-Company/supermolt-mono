{
  "name": "sr-mobile",
  "version": "0.1.0",
  "private": true,
  "description": "SR-Mobile - AI Trading Agent for Solana Seeker",
  "scripts": {
    "mobile": "pnpm --filter mobile",
    "backend": "pnpm --filter backend",
    "web": "pnpm --filter web",
    "dev:mobile": "pnpm --filter mobile start",
    "dev:backend": "pnpm --filter backend dev",
    "dev:web": "pnpm --filter web dev",
    "dev": "pnpm run --parallel dev:backend dev:web",
    "build": "pnpm run build:backend && pnpm run build:web",
    "build:backend": "pnpm --filter backend build",
    "build:web": "pnpm --filter web build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck"
  },
  "keywords": [
    "solana",
    "mobile",
    "trading",
    "ai-agent",
    "mwa"
  ],
  "author": "",
  "license": "MIT"
}
